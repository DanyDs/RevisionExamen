
public class NewReleaseMovie extends  {

	public NewReleaseMovie(String title, int priceCode) {
		super(title, priceCode);
		// TODO Auto-generated constructor stub
	}

	@Override
	int calculateDaysRentedAmount(int amount, Rental each) {
		amount += each.getDaysRented() * 3;
		return amount;
	}

}
